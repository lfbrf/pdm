<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent">


    <TextView
        android:id="@+id/txtNomeListaConsultaM"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginRight="10dp"
        android:text="Medico" />

    <TextView
        android:id="@+id/txtNomeListaConsultaU"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginRight="10dp"
        android:layout_toEndOf="@+id/txtNomeListaConsultaM"
        android:layout_toRightOf="@+id/txtNomeListaConsultaM"
        android:text="Usuario" />

    <TextView
        android:id="@+id/txtConsultaData"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_marginRight="10dp"
        android:layout_toEndOf="@+id/txtNomeListaConsultaU"
        android:layout_toRightOf="@+id/txtNomeListaConsultaU"
        android:text="Data" />


</RelativeLayout>